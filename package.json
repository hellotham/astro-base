{
  "name": "astro-base",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -rf node_modules .astro dist",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build; prettier --write  \"dist/**/*.{js,html,css}\"",
    "preview": "astro preview",
    "lint": "pnpm run lint:prettier && pnpm run lint:eslint",
    "lint:prettier": "prettier --write  \"src/**/*.{js,jsx,ts,tsx,md,mdx,astro}\"",
    "lint:eslint": "eslint --fix \"src/**/*.{js,ts,jsx,tsx,md,mdx,astro}\"",
    "refresh": "pnpm dlx @astrojs/upgrade; pnpm up -L"
  },
  "dependencies": {
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.5.1",
    "astro": "^5.13.3"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@eslint/markdown": "^7.2.0",
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/logos": "^1.2.9",
    "@julr/unocss-preset-forms": "^2.0.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@unocss/eslint-config": "^66.4.2",
    "eslint": "^9.34.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-mdx": "^3.6.2",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "unocss": "^66.4.2",
    "vite": "^7.1.3"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
